#!/usr/bin/env -S mycmd
# -*- mode: shell-script; sh-shell: bash; sh-basic-offset: 4; sh-indentation: 4; coding: utf-8 -*-
# shellcheck shell=bash

# HELP START
# list all original source financal data CSV files, ordered by date
#
# This will list all of the original financial CSV files found, grouped
# by account and ordered by date.
# HELP END

set -o nounset -o errexit -o errtrace -o pipefail

function mycmd.main() {
    if (($# < 1)); then
        mycmd.trace "Listing all CSVs"

        financial.list_chase_visa_source_csvs_by_date
        financial.list_capital_one_money_market_source_csvs_by_date
        financial.list_capital_one_checking_source_csvs_by_date
    fi
}
